<?xml version="1.0" encoding="UTF-8" ?>
<container  xmlns="http://symfony.com/schema/dic/services"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:symfony="http://symfony.com/schema/dic/symfony"
            xsi:schemaLocation="
                http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd
                http://symfony.com/schema/dic/symfony https://symfony.com/schema/dic/symfony/symfony-1.0.xsd
            "
>
    <symfony:config default-locale="en">
        <symfony:translator enabled="true" fallback="en">
            <symfony:path>%kernel.project_dir%/src/Carthage/Resources/Shared/Translation</symfony:path>
            <symfony:path>%kernel.project_dir%/src/Carthage/Resources/LogManagement/Translation</symfony:path>
            <symfony:path>%kernel.project_dir%/src/Carthage/Resources/MetricCollection/Translation</symfony:path>
        </symfony:translator>
    </symfony:config>
</container>
