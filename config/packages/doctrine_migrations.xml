<?xml version="1.0" encoding="UTF-8" ?>
<container  xmlns="http://symfony.com/schema/dic/services"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:doctrine="http://symfony.com/schema/dic/doctrine/migrations/3.0"
            xsi:schemaLocation="
                http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd
                http://symfony.com/schema/dic/doctrine/migrations/3.0 https://symfony.com/schema/dic/doctrine/migrations/doctrine_migrations-3.0.xsd
            "
>
    <doctrine:config>
        <doctrine:migrations-path namespace="DoctrineMigrations">%kernel.project_dir%/src/Carthage/Resources/Shared/Migration</doctrine:migrations-path>
    </doctrine:config>
</container>
