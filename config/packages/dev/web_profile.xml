<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:symfony="http://symfony.com/schema/dic/symfony"
           xmlns:webprofiler="http://symfony.com/schema/dic/webprofiler"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd
            http://symfony.com/schema/dic/symfony https://symfony.com/schema/dic/symfony/symfony-1.0.xsd
            http://symfony.com/schema/dic/webprofiler https://symfony.com/schema/dic/webprofiler/webprofiler-1.0.xsd">

    <webprofiler:config toolbar="true" intercept-redirects="false"/>

    <symfony:config>
        <symfony:profiler only-exceptions="false" collect-serializer-data="true"/>
    </symfony:config>
</container>
